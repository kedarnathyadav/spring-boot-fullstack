{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "kedarnath-api",
      "image": "dkedarnath/kedarnath-api:latest",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8080
        }
      ],
      "environment": [
        {
          "name": "SPRING_DATASOURCE_URL",
          "value": "jdbc:postgresql://awseb-e-w8wmuiq5dw-stack-awsebrdsdatabase-chpemc4crsmm.c1qawm2oqlx3.us-east-1.rds.amazonaws.com:5432/customer"
        }
      ],
      "healthCheck": {
        "command": [
          "CMD-SHELL",
          "curl -f http://localhost:8080/health || exit 1"
        ],
        "interval": 30,
        "timeout": 5,
        "retries": 3,
        "startPeriod": 60
      }
    }
  ]
}